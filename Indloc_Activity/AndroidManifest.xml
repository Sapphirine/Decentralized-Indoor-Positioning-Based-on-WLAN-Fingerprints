<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="indloc.indoor_location_2">
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:allowBackup="true" android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="indloc.indoor_location_2.activity.Indloc_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="indloc.indoor_location_2.activity.Data_collect_Activity"/>
        <activity android:name="indloc.indoor_location_2.activity.Data_manage_Activity"/>
        <activity android:name="indloc.indoor_location_2.activity.Data_process_Activity"/>
        <activity android:name="indloc.indoor_location_2.activity.Location_Activity"/>
    </application>
</manifest>
